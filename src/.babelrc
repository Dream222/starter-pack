{
  "presets": [
    [
      "latest",
      {
        "es2015": {
          "modules": false,
          "loose": true
        }
      }
    ],
    "react"
  ],
  "plugins": [
    "syntax-dynamic-import",
    [
      "react-loadable/babel",
      { "webpack": true }
    ],
    "transform-inline-environment-variables",
    "transform-class-properties",
    "transform-object-rest-spread"
  ]
}
